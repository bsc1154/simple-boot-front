    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.44.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
