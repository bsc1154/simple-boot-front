{
  "name": "simple-boot-front",
  "version": "1.0.10",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "front end SPA frameworks",
  "keywords": [
    "front",
    "spa",
    "SPA",
    "framwork",
    "node"
  ],
  "homepage": "https://github.com/visualkhh/simple-boot-front",
  "bugs": {
    "url": "https://github.com/visualkhh/simple-boot-front",
    "email": "visualkhh@gmail.com"
  },
  "author": {
    "name": "visualkhh",
    "email": "visualkhh@gmail.com",
    "url": "https://github.com/visualkhh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visualkhh/simple-boot-front"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --outDir dist --declarationDir dist",
    "build2": "rm -rf dist && tsc --outDir dist/lib --declarationDir dist/types",
    "build-rollup": "rm -rf dist && tsc --outDir dist/lib --declarationDir dist/types --declaration true && rollup -c",
    "bundle2": "rm -rf dist && tsc --declarationDir dist/types && rollup -c",
    "bundle": "rm -rf dist && rollup -c",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.44.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4"
  },
  "dependencies": {
    "core-js": "^3.9.1",
    "handlebars": "^4.7.7",
    "npm": "^7.5.4",
    "rollup-plugin-multi-input": "^1.2.0",
    "rxjs": "6.0.0",
    "rxjs-compat": "6.2.2",
    "typescript": "^4.2.3"
  }
}
